{
  "name": "finfix-bot",
  "version": "1.1.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development node --loader ts-node/esm src/index.ts",
    "feedback": "NODE_ENV=development node --loader ts-node/esm scripts/viewFeedback.ts",
    "feedback:mark-read": "NODE_ENV=development node --loader ts-node/esm scripts/viewFeedback.ts --mark-read",
    "sync-db": "NODE_ENV=development node --loader ts-node/esm scripts/syncDatabase.ts",
    "migrate-feedback": "NODE_ENV=development node --loader ts-node/esm scripts/migrateFeedback.ts",
    "add-category": "NODE_ENV=development node --loader ts-node/esm scripts/addCategoryToFeedbacks.ts",
    "migrate-mono": "NODE_ENV=development node --loader ts-node/esm scripts/runMigration.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [],
  "author": "illiacodes",
  "license": "ISC",
  "description": "Finfix telegram bot",
  "dependencies": {
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "telegraf": "^4.16.3",
    "telegraf-session-local": "^2.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
